#include<stdio.h>
int fib(int n)
{
	if(n==0)
	{
		return 0;
	}
	else if(n=1)
	{
		return 1;
	}
	else
	{
		return(fib(n-1)+fib(n-2));
		
	}
}
	int main()
	{
		int n,i,ft,st,sum;
		printf("enter N value:");
		scanf("%d",&n);
		printf("enter values:");
		for(i=0;i<n;i++)
		{
			printf("%d\t",fib(i));
			i=2;    
    while (i<n)
    {
        sum=ft+st;
        ft=st;
        st=sum;
        ++i;
        printf("%d ",sum);
    }
			
		}
		
	}
